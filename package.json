{
  "name": "cart-service",
  "version": "1.0.0",
  "description": "Shopping cart microservice for xshopai - Node.js/TypeScript implementation",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "npx tsx watch src/server.ts",
    "debug": "npx tsx --inspect=9229 src/server.ts",
    "start:dapr": "dapr run --app-id cart-service --app-port 8008 --dapr-http-port 3508 --dapr-grpc-port 50008 --log-level warn --resources-path ./.dapr/components --config ./.dapr/config.yaml -- node dist/server.js",
    "dev:dapr": "dapr run --app-id cart-service --app-port 8008 --dapr-http-port 3508 --dapr-grpc-port 50008 --log-level warn --resources-path ./.dapr/components --config ./.dapr/config.yaml -- npx tsx watch src/server.ts",
    "debug:dapr": "dapr run --app-id cart-service --app-port 8008 --dapr-http-port 3508 --dapr-grpc-port 50008 --log-level warn --resources-path ./.dapr/components --config ./.dapr/config.yaml -- npx tsx --inspect=9229 src/server.ts",
    "build": "tsc",
    "test": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js --runInBand",
    "test:unit": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js --runInBand tests/unit",
    "test:integration": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js --runInBand tests/integration",
    "test:e2e": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js --runInBand tests/e2e",
    "test:watch": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js --coverage --runInBand",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "cart",
    "shopping",
    "microservice",
    "typescript",
    "dapr"
  ],
  "author": "xshopai Team",
  "license": "MIT",
  "dependencies": {
    "@dapr/dapr": "^3.6.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.56.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.56.0",
    "@opentelemetry/resources": "^2.5.0",
    "@opentelemetry/sdk-node": "^0.211.0",
    "@opentelemetry/semantic-conventions": "^1.39.0",
    "cors": "^2.8.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "ioredis": "^5.4.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.34.0",
    "jest": "^30.0.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.0",
    "tsx": "^4.19.0",
    "typescript": "^5.5.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
